{
  "name": "Logger",
  "version": "0.0.1",
  "description": "Just a js logger",
  "keywords": [
    "logger",
    "library",
    "rollup",
    "typescript"
  ],
  "homepage": "https://github.com/boazhoch/Logger",
  "bugs": {
    "url": "https://github.com/boazhoch/Loggerissues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boazhoch/Logger.git"
  },
  "license": "MIT",
  "author": "Boaz hoch boazmier@gmail.com",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "types/index.cjs.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prebuild": "npm run test && npm run clean",
    "build": "npm run build:js",
    "build:js": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint src/**/* --fix",
    "prerelease": "npm run build",
    "release": "npx standard-version",
    "postrelease": "git push --follow-tags",
    "start": "rollup -c -w",
    "pretest": "npm run lint",
    "test": "jest",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch --notify",
    "types:check": "tsc --noEmit",
    "types:watch": "npm run types:check -- --watch"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@wessberg/rollup-plugin-ts": "^1.3.7",
    "eslint": "^7.12.1",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.1",
    "koa-body": "^4.2.0",
    "rollup": "^2.32.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
